#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/quan/Documents/Blog/node_modules/.pnpm/vite@6.3.6_@types+node@24.3.1_jiti@2.5.1_lightningcss@1.29.3_terser@5.39.0_tsx@4.20.5_yaml@2.8.1/node_modules/vite/bin/node_modules:/Users/quan/Documents/Blog/node_modules/.pnpm/vite@6.3.6_@types+node@24.3.1_jiti@2.5.1_lightningcss@1.29.3_terser@5.39.0_tsx@4.20.5_yaml@2.8.1/node_modules/vite/node_modules:/Users/quan/Documents/Blog/node_modules/.pnpm/vite@6.3.6_@types+node@24.3.1_jiti@2.5.1_lightningcss@1.29.3_terser@5.39.0_tsx@4.20.5_yaml@2.8.1/node_modules:/Users/quan/Documents/Blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/quan/Documents/Blog/node_modules/.pnpm/vite@6.3.6_@types+node@24.3.1_jiti@2.5.1_lightningcss@1.29.3_terser@5.39.0_tsx@4.20.5_yaml@2.8.1/node_modules/vite/bin/node_modules:/Users/quan/Documents/Blog/node_modules/.pnpm/vite@6.3.6_@types+node@24.3.1_jiti@2.5.1_lightningcss@1.29.3_terser@5.39.0_tsx@4.20.5_yaml@2.8.1/node_modules/vite/node_modules:/Users/quan/Documents/Blog/node_modules/.pnpm/vite@6.3.6_@types+node@24.3.1_jiti@2.5.1_lightningcss@1.29.3_terser@5.39.0_tsx@4.20.5_yaml@2.8.1/node_modules:/Users/quan/Documents/Blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vite@6.3.6_@types+node@24.3.1_jiti@2.5.1_lightningcss@1.29.3_terser@5.39.0_tsx@4.20.5_yaml@2.8.1/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vite@6.3.6_@types+node@24.3.1_jiti@2.5.1_lightningcss@1.29.3_terser@5.39.0_tsx@4.20.5_yaml@2.8.1/node_modules/vite/bin/vite.js" "$@"
fi
